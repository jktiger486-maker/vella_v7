```python
# ============================================================
# AUTO-GENERATED FILE â€” DO NOT EDIT MANUALLY
# SOURCE OF TRUTH: PIT-5, PIT-6, CODEGEN-13
# ============================================================

from typing import Optional
from vella_v7.contracts.contracts import (
    ContextSnapshot,
    EntrySignal,
    EngineState,
)


# ------------------------------------------------------------
# Strategy constants (v1)
# ------------------------------------------------------------
MIN_VOLATILITY = 0.0   # minimal non-zero activity gate


def generate_entry_signal(
    context: ContextSnapshot,
    state: EngineState,
) -> Optional[EntrySignal]:
    """
    Entry Strategy v1 (Minimal)

    Rules:
    - One condition
    - No optimization
    """

    # --------------------------------------------------------
    # Guard: no re-entry
    # --------------------------------------------------------
    if state.has_position:
        return None

    # --------------------------------------------------------
    # Rule v1: minimal volatility gate
    # --------------------------------------------------------
    if context.volatility <= MIN_VOLATILITY:
        return None

    # --------------------------------------------------------
    # Emit entry signal (fixed direction)
    # --------------------------------------------------------
    return EntrySignal(
        side="SHORT",
        confidence=0.2,
        reason="ENTRY_V1: volatility gate passed",
    )


